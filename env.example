# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Firebase Cloud Messaging - Web Push (Optional for web browser notifications)
# Get from: Firebase Console > Project Settings > Cloud Messaging > Web Push certificates
VITE_FIREBASE_VAPID_KEY=your_vapid_key_starts_with_BH

# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# EmailJS Configuration
VITE_EMAILJS_SERVICE_ID=your_emailjs_service_id
VITE_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
VITE_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# PostHog Analytics
VITE_POSTHOG_API_KEY=phc_your_posthog_api_key_here
VITE_POSTHOG_HOST=https://app.posthog.com

# SendGrid Email
VITE_SENDGRID_API_KEY=SG.your_sendgrid_api_key_here
VITE_FROM_EMAIL=noreply@yourcompany.com

# Other API Keys
VITE_WEATHER_API_KEY=your_openweathermap_api_key
VITE_SMS_API_KEY=your_sms_service_api_key
